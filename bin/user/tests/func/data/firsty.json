            }  
          ]
        },
        {
          "type": "keyword",
          "units": 17,
          "records": [
            {
              "dateTime": null,
              "usUnits": 17,
              "inTemp": 0,
              "outTemp": 1
            }   
          ]
        }          
      ]
    }
  
}
    

