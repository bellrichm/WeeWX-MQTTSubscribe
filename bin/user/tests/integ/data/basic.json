{
	"comments": [
		"Basic test,",
		"Run through the WeeWX acuumulator."
	],
	"config": [
		"[MQTTSubscribeService]",
		"    # Configuration for the message callback.",
		"    [[message_callback]]",
		"    # The topics to subscribe to.",
		"    [[topics]]",
		"        unit_system = METRICWX",
		"        [[[weewx-$test_id/#]]]",
		"            [[[[weewx-$test_id/inTemp]]]]",
		"                name = inTemp",
		"            [[[[weewx-$test_id/outTemp]]]]",
		"                name = outTemp"		
	],
	"testruns": [
		{
			"messages": [
				{
					"weewx-$test_id": {
						"separator": ",",
						"delimiter": "=",
						"data": {
							"inTemp": 0,
							"outTemp": 1
						}
					}
				}
			],
			"results": [
				{
					"test": ["single", "driver"],
					"payloads": ["json", "keyword"],
					"units": 17,
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},
				{
					"test": ["accumulate"],
					"payloads": ["json", "keyword"],
					"units": 17,
					"records": [
						{
						},
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},				
				{
					"test": ["service"],
					"payloads": ["json", "keyword"],
					"units": 17,
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},				
				{
					"test": ["accumulate"],
					"payloads": ["individual"],
					"units": 17,
					"records": [
						{
						},
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},
				{
					"test": ["service"],
					"payloads": ["individual"],
					"units": 17,
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},
				{
					"test": ["single", "driver"],
					"payloads": ["individual"],
					"units": 17,
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0
						},
						{
							"dateTime": null,
							"usUnits": 17,
							"outTemp": 1
						}
					]
				}
			]
		}
	]
}
