{
	"comments": [
		"Basic test,",
		"Run through the WeeWX acuumulator."
	],
	"types": [
		"individual",
		"json",
		"keyword"
	],
	"config": [
		"[MQTTSubscribeService]",
		"    # Configuration for the message callback.",
		"    [[message_callback]]",
		"       [[[label_map]]]",
		"          baro_mb = barometer",
		"          windRaw = windSpeed",
		"          ",
		"        [[[fields]]]",
		"            # The incoming field name from MQTT",
		"            [[[[windRaw]]]]",
		"              # The WeeWX name.",
		"              # Default is the name from MQTT.",
		"                name = windSpeed",
		"              # The conversion type necessary for WeeWX compatibility",
		"              # Valid values: bool, float, int, none",
		"              # Default is float",
		"                conversion_type = float",
		"              # True if the incoming data is cumulative.",
		"              # Valid values: True, False",
		"              # Default is False",
		"                contains_total = False",
		"                ",
		"    # The topics to subscribe to.",
		"    [[topics]]",
		"        unit_system = METRICWX",
		"        [[[weewx/#]]]"
	],
	"testruns": [
		{
			"messages": [
				{
					"weewx": {
						"separator": ",",
						"delimiter": "=",
						"data": {
							"inTemp": 0,
							"outTemp": 1
						}
					}
				}
			],
			"results": [
				{
					"test": "both",
					"payloads": [
						"json",
						"keyword"
					],
					"units": 17,
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},
				{
					"test": "accumulate",
					"payloads": [
						"individual"
					],
					"units": 17,
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0,
							"outTemp": 1
						}
					]
				},
				{
					"test": "single",
					"payloads": [
						"individual"
					],
					"records": [
						{
							"dateTime": null,
							"usUnits": 17,
							"inTemp": 0
						},
						{
							"dateTime": null,
							"usUnits": 17,
							"outTemp": 1
						}
