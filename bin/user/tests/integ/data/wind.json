{
  "comments": [
    "Wind data collected into a packet,"
  ],
  "config": [
    "[MQTTSubscribeService]",
    "    # Configuration for the message callback.",
    "    [[message_callback]]",
    "    # The topics to subscribe to.",
    "    [[topics]]",
    "        unit_system = METRICWX",
    "        collect_wind_across_loops = False",
    "        [[[weewx-$test_id/#]]]",
    "            [[[[weewx-$test_id/windDir]]]]",
    "                name = windDir",
    "            [[[[weewx-$test_id/windSpeed]]]]",
    "                name = windSpeed"
  ],
  "testruns": [
    {
      "messages": [
        {
          "weewx-$test_id": {
            "separator": ",",
            "delimiter": "=",
            "data": {
              "windDir": 0,
              "windSpeed": 1
            }
          }
        }
      ],
      "results": [
        {
          "test": ["single", "driver"],
          "payloads": ["json", "keyword", "individual"],
          "units": 17,
